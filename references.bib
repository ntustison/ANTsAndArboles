  Mesh                     = {Algorithms; Computer Graphics; Computer Simulation; Database Management Systems; Databases, Factual; Image Interpretation, Computer-Assisted; Imaging, Three-Dimensional; Models, Theoretical; User-Computer Interface},
  Pmid                     = {22508905},
